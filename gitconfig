[user]
	name = F
	email = fwrizalman@gmail.com
[core]
  excludesfile = /Users/faezrah/.gitignore_global
[alias]
  c = commit -m
  a = add
  pl = pull
  ps = push
  st = status
  br = branch
  type = cat-file -t
  dump = cat-file -p
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  ac = commit -am
  pr = pull --rebase
  prm = pull --rebase origin master
  lone = log --oneline
  co = checkout
  m = merge
  s = stash
  sp = stash pop
  sl = stash list
  cb = checkout -b
  psm = push origin master
  amend = commit --amend -m
  psf = push origin dev.faezrah
  prf = pull --rebase origin dev.faezrah
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)%Creset' --abbrev-commit
  undo = reset --soft HEAD~1
  showfiles = show --name-only
  force = push --force
  count = rev-list HEAD --count
[color]
  ui = true
[color "branch"]
  remote = white reverse
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  new = green bold
[color "status"]
  added = yellow bold
  changed = green
  untracked = cyan
[filter "lfs"]
  clean = git lfs clean %f
  smudge = git lfs smudge %f
  required = true
[http]
  sslVerify = true